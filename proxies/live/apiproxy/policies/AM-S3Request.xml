<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-S3Request">
    <DisplayName>AM-S3Req</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>aws-service-name</Name>
        <Value>s3</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>aws-service-endpoint</Name>
        <Value>booking-and-referral-targets-int.s3.amazonaws.com</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>aws-region</Name>
        <Value>eu-west-2</Value>
    </AssignVariable>
    <Set>
        <Verb>PUT</Verb>
        <Path>newS3ObjectKey</Path>
        <Headers>
            <Header name="content-type">application/octet-stream</Header>
        </Headers>
        <Payload>New S3 Value</Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="new" transport="http" type="request">s3Callout</AssignTo>
</AssignMessage>