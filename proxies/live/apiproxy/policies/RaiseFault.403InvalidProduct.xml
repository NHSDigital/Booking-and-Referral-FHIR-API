<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RaiseFault.403InvalidProduct">
    <DisplayName>RaiseFault.403InvalidProduct</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="text/plain"/>
            <StatusCode>403</StatusCode>
            <ReasonPhrase>Forbidden</ReasonPhrase>
        </Set>
        <AssignVariable>
            <Name>bref.error.code</Name>
            <Value>value</Value>
        </AssignVariable>
        <AssignVariable>
            <Name>bref.error.coding.code</Name>
            <Value>SEND_FORBIDDEN</Value>
        </AssignVariable>
        <AssignVariable>
            <Name>bref.error.diagnostics</Name>
            <Value>Invalid Product</Value>
        </AssignVariable>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>