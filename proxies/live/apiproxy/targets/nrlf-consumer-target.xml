<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="nrlf-consumer-target">
  <Description/>
  <FaultRules/>
  <PreFlow name="PreFlow">
    <Request>
      <Step>
        <Name>KVM.GetNrlfPrivateKey</Name>
      </Step>
      <Step>
        <Name>GenerateJWT.NrlfClientAssertion</Name>
      </Step>
      <Step>
        <Name>ServiceCallout.NrlfAuth</Name>
      </Step>
      <Step>
        <Name>Javascript.NrlfAddAuthorization</Name>
      </Step>
      <Step>
        <Name>AssignMessage.NrlfRequest</Name>
      </Step>
    </Request>
    <Response/>
  </PreFlow>
  <LocalTargetConnection>
    <APIProxy>nrl-consumer-api-{{ APIGEE_ENVIRONMENT }}</APIProxy>
    <ProxyEndpoint>default</ProxyEndpoint>
  </LocalTargetConnection>
</TargetEndpoint>
